{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es2016",                                
    "module": "commonjs",                             
    "esModuleInterop": true,                            
    "forceConsistentCasingInFileNames": true,            
    "strict": false,                                    
    "skipLibCheck": true
    ,
    
    
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    
  },
  "include" :["src","./"]
}
